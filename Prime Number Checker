function main(){
var num = Number(prompt(""))
console.log(isprime(num))
}
function isprime(num){
    var isprime=true
for(i=2;i<=num/2;i++)
{  
    if(num%i==0){
    isprime=false
    break
    }
} 
if(isprime)
{
   return num+ " is a prime number"
}else
{
    return num+ " is not a prime number"
}
}
main()
