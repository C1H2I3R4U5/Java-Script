const secondImportant = function(a,b,c){
   // Your code goes here
   var largestNumber = Math.max(a,b,c)
   var SecondLargestNumber =Math.max(Math.min(a,b),Math.min(b,c),Math.min(a,c))
   return SecondLargestNumber
   
};
